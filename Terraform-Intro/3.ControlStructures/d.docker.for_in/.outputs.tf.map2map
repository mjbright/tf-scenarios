
locals {
    vms_names = keys( var.vms )

}

## -- Map  -> Map  transformations: -----------------------------

output map2map {
    value = { for key, val in var.vms : key => val["ext_port"] }
}

output map2map_filter {
    value = { for key, val in var.vms : key => val["ext_port"] if val["ext_port"] == 8002 }
}


