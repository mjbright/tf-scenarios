
locals {
    vms_names = keys( var.vms )

}

## -- Map  -> List transformations: -----------------------------

output map2list {
    # equivalent to value = keys( var.vms )
    value = [ for key, val in var.vms : key ]
}

output map2list_filter {
    # equivalent to value = keys( var.vms )
    value = [ for key, val in var.vms : key if key == "vm2" ]
}


